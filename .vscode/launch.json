{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm run test",
            "name": "npm run test",
            "request": "launch",
            "type": "node-terminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceRoot}",
            "sourceMaps": true,
        },
        {
            "name": "Current JS File",
            "type": "node",
            "request": "launch",
            "args": [
                "${relativeFile}"
            ],
            "runtimeArgs": [
                "--nolazy"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
        },
        {
            "name": "Current TS File",
            "type": "node",
            "request": "launch",
            "args": [
                "${relativeFile}"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
        },
        {
            "name": "cmd: index 2022 1",
            "type": "node",
            "request": "launch",
            "args": [
                "src/bin.ts",
                "index",
                "2022",
                "1"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
        },
        {
            "name": "cmd: index 2024 1 em",
            "type": "node",
            "request": "launch",
            "args": [
                "src/bin.ts",
                "index",
                "2024",
                "1",
                "em"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
        },
        {
            "name": "cmd: refresh leaderboard w/ ID",
            "type": "node",
            "request": "launch",
            "args": [
                "src/bin.ts",
                "refresh",
                "leaderboard",
                "2024",
                "--leaderboard-id",
                "1737071"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
        },
        {
            "name": "cmd: refresh leaderboard w/o ID",
            "type": "node",
            "request": "launch",
            "args": [
                "src/bin.ts",
                "refresh",
                "leaderboard",
                "2024"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
        },
        {
            "name": "cmd: stats-print",
            "type": "node",
            "request": "launch",
            "args": [
                "src/bin.ts",
                "stats-print",
                "2024"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
        },
        {
            "name": "cmd: stats-sync",
            "type": "node",
            "request": "launch",
            "args": [
                "src/bin.ts",
                "stats-sync",
                "2024"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
        },
        {
            "name": "cmd: leaderboard-to-csv",
            "type": "node",
            "request": "launch",
            "args": [
                "src/bin.ts",
                "leaderboard-to-csv",
                "2024"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
        },
        {
            "name": "argsToOptions",
            "type": "node",
            "request": "launch",
            "args": [
                "src/options.ts",
                "--tests-only",
                "--no-force-submit",
                "--part",
                "1"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
        }
    ]
}